{
  "name": "practical-react-components-core",
  "version": "0.36.0",
  "description": "Practical react components library - Core",
  "homepage": "https://github.com/AxisCommunications/practical-react-components",
  "license": "MIT",
  "main": "build/index.js",
  "files": [
    "build"
  ],
  "dependencies": {
    "@juggle/resize-observer": "3.3.1",
    "focus-trap-react": "8.7.0",
    "no-scroll": "2.1.1",
    "practical-react-components-icons": "workspace:*",
    "react-hooks-shareable": "1.32.0"
  },
  "devDependencies": {
    "@babel/cli": "7.14.5",
    "@babel/core": "7.14.6",
    "@babel/preset-env": "7.14.7",
    "@babel/preset-react": "7.14.5",
    "@babel/preset-typescript": "7.14.5",
    "@mdx-js/mdx": "1.6.22",
    "@mdx-js/react": "1.6.22",
    "@testing-library/dom": "7.31.2",
    "@testing-library/jest-dom": "5.12.0",
    "@testing-library/react": "12.0.0",
    "@testing-library/user-event": "13.1.9",
    "@types/jest": "26.0.23",
    "@types/no-scroll": "2.1.0",
    "@types/react": "17.0.13",
    "@types/react-dom": "17.0.8",
    "@types/react-test-renderer": "17.0.1",
    "@types/react-transition-group": "4.4.1",
    "@types/styled-components": "5.1.11",
    "@types/testing-library__dom": "7.5.0",
    "@types/testing-library__jest-dom": "5.9.5",
    "@types/testing-library__user-event": "4.2.0",
    "@types/yup": "0.29.12",
    "babel-jest": "27.0.6",
    "babel-plugin-styled-components": "1.13.1",
    "core-js": "3.15.2",
    "jest": "27.0.6",
    "jest-styled-components": "7.0.4",
    "pepjs": "0.5.3",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "17.0.2",
    "react-test-renderer": "17.0.2",
    "react-transition-group": "4.4.2",
    "styled-components": "5.3.0",
    "typeface-open-sans": "1.1.13",
    "typescript": "4.3.5",
    "yup": "0.32.9"
  },
  "peerDependencies": {
    "pepjs": "^0.5.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-transition-group": "^4.3.0",
    "styled-components": "^5.1.0"
  },
  "scripts": {
    "tsc": "tsc --emitDeclarationOnly --noEmit false",
    "build:ts": "babel src --out-dir build --root-mode upward --ignore \"**/*.test.tsx\" --extensions .ts,.tsx",
    "build": "yarn build:ts && yarn tsc",
    "dev": "yarn build:ts --watch && yarn tsc "
  },
  "jest": {
    "testMatch": [
      "**/packages/**/?(*.)test.ts?(x)"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!@juggle)"
    ],
    "transform": {
      "\\.(js|ts)x?$": "babel-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "testEnvironment": "jsdom"
  }
}
